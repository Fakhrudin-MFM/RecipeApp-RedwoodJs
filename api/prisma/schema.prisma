datasource DS {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

// Define your own datamodels here and run `yarn redwood db save` to create
// migrations for them.
model Recipe {
  id          Int     @id @default(autoincrement())
  name        String  @unique
  description String
  image_url   String?
}
